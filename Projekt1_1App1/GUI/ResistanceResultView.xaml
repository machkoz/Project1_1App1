<UserControl x:Class="Projekt1_1App1.GUI.ResistanceResultView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Projekt1_1App1.GUI"
             mc:Ignorable="d"              >
    <Grid >
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Label  Width="72" Content="{Binding Title1, FallbackValue=nie udało się}"/>
            <Label Width="47" Content="R(p) = "/>
            <Label  Width="92" Content="{Binding Resistor1, FallbackValue=rezystancja1}"/>
            <Label  Width="50" Content="R1(p) = "/>
            <Label Width="92" Content="{Binding Resistor2, FallbackValue=rezystancja2}"/>
            <Label Width="60" Content="U(wy/p) = "/>
            <Label Width="92" Content="{Binding VoltageOut, FallbackValue=napięcie wyjściowe}"/>
            <Label Width="70" Content="Rez.zast. = "/>
            <Label Width="92" Content="{Binding SupplementaryResistance, FallbackValue=rezystancja zastępcza}"/>
        </StackPanel>
    </Grid>
</UserControl>
